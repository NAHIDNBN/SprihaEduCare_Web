    }

    .programImg {
        transition: transform 0.5s;
    }

        .programImg:hover {
            transform: scale(1.05);
        }

    /*...Sliding Images...*/
    @@keyframes slide {
        from {
            transform: translateX(0);
        }

        to {
            transform: translateX(-100%);
        }
    }

    .logos {
        overflow: hidden;
        /*padding: 60px 0;*/
        white-space: nowrap;
        position: relative;
    }

        .logos:before,
        .logos:after {
            position: absolute;
            top: 0;
            /*width: 250px;*/
            height: 100%;
            content: "";
            z-index: 2;
        }

